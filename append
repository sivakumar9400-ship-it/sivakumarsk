import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        String S1 = sc.nextLine();
        String S2 = sc.nextLine();
        
        String result = S1.concat(S2);  
        
        System.out.println(result);
        
    
    }
}
